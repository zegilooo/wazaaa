{
  "name": "wazaaa",
  "version": "1.0.0",
  "description": "Demo app used in Delicious Insightsâ€™ Node.js training course",
  "private": true,
  "engines": {
    "node": ">= 6.0.0",
    "npm": ">= 3.8.0"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack",
    "debug": "iron-node boot.js",
    "dev:server": "nodemon boot.js",
    "dev:client": "webpack --watch",
    "dev:test": "npm run test -- --watch",
    "doc": "rimraf doc && groc",
    "lint": "standard --verbose | snazzy",
    "postinstall": "rimraf .jshintrc .jshintignore && npm run build",
    "start": "node boot.js",
    "test": "cross-env NODE_ENV=test mocha",
    "test:cov": "nyc --reporter=lcov npm test && rimraf .nyc_output",
    "validate": "npm test && npm ls"
  },
  "babel": {
    "presets": ["node6"],
    "plugins": ["transform-object-rest-spread"]
  },
  "standard": {
    "env": {
      "node": true,
      "mocha": true
    },
    "parser": "babel-eslint",
    "ignore": [
      "/doc/"
    ]
  },
  "pre-commit": [
    "lint",
    "test"
  ],
  "keywords": [
    "node",
    "express",
    "connect",
    "passport",
    "social",
    "demo",
    "training",
    "jade",
    "webpack",
    "mocha",
    "tests"
  ],
  "author": "Christophe Porteneuve <christophe@delicious-insights.com> (http://delicious-insights.com)",
  "license": "No-License (see LICENSE file)",
  "dependencies": {
    "autoprefixer": "^6.3.1",
    "babel-core": "^6.9.1",
    "babel-eslint": "^6.0.0",
    "babel-loader": "^6.2.2",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-node6": "^11.0.0",
    "body-parser": "^1.15.2",
    "bootstrap": "^3.3.6",
    "colors": "^1.0.3",
    "connect-flash": "^0.1.1",
    "cookie-session": "^2.0.0-alpha.1",
    "cross-env": "^1.0.8",
    "css-loader": "^0.23.1",
    "csurf": "^1.9.0",
    "errorhandler": "^1.3.0",
    "express": "^4.14.0",
    "file-loader": "^0.8.5",
    "hjs-webpack": "^8.3.0",
    "isomorphic-fetch": "^2.2.1",
    "jade": "^1.8.1",
    "jade-loader": "^0.8.0",
    "jquery": "^2.2.4",
    "method-override": "^2.3.6",
    "moment": "^2.8.4",
    "mongoose": "^4.5.1",
    "morgan": "^1.5.0",
    "passport": "^0.3.2",
    "passport-facebook": "^2.1.1",
    "passport-twitter": "^1.0.2",
    "postcss-loader": "^0.9.1",
    "rimraf": "^2.5.2",
    "select2": "3.5.2-browserify",
    "socket.io": "^1.2.1",
    "style-loader": "^0.13.0",
    "stylus": "^0.54.2",
    "stylus-loader": "^2.1.1",
    "underscore": "^1.7.0",
    "unfluff": "^1.0.0",
    "url-loader": "^0.5.7"
  },
  "devDependencies": {
    "bson-objectid": "^1.1.4",
    "chai": "^3.5.0",
    "faker": "^3.1.0",
    "groc": "^0.8.0",
    "iron-node": "^3.0.6",
    "mocha": "^2.5.3",
    "nock": "^8.0.0",
    "nodemon": "^1.9.1",
    "nyc": "^6.6.1",
    "precommit-hook": "^3.0.0",
    "sinon": "^1.17.3",
    "sinon-chai": "^2.8.0",
    "snazzy": "^4.0.0",
    "standard": "^7.1.2",
    "supertest": "^1.2.0"
  }
}
